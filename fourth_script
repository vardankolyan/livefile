#!/bin/bash
curl -q https://stevessmarthomeguide.com/internal-external-ip-addresses/ --output ./url



ip="\([1-9]\|[1-9][0-9]\|1[0-9][0-9]\|2[0-4][0-9]\|25[0-5]\)\.\([0-9]\|[1-9][0-9]\|1[0-9][0-9]\|2[0-4][0-9]\|25[0-5]\)\.\([0-9]\|[1-9][0-9]\|1[0-9][0-9]\|2[0-4][0-9]\|25[0-5]\)\.\([0-9]\|[1-9][0-9]\|1[0-9][0-9]\|2[0-4][0-9]\|25[0-4]\)"

int_10="10\(\.\([0-9]\|[1-9][0-9]\|1[0-9][0-9]\|2[0-4][0-9]\|25[0-5]\)\)\{3\}"

int_192="192\.168\(\.\([0-9]\|[1-9][0-9]\|1[0-9][0-9]\|2[0-4][0-9]\|25[0-5]\)\)\{2\}"

int_172="172\.\(1[6-9]\|2[0-9]\|3[0-1]\)\(\.\([0-9]\|[1-9][0-9]\|1[0-9][0-9]\|2[0-4][0-9]\|25[0-5]\)\)\{2\}"

grep -o "$ip" url > ip
echo "10.0.0.5" >> ip
echo "192.168.10.5" >> ip
echo "172.16.5.3" >> ip
grep -e "$int_10"  -e "$int_192" -e "$int_172" ip > internal
grep -ve "$int_10" -e "$int_192" -e "$int_172" ip > external

grep -Eo "https?://?(www.)?[-a-zA-Z0-9@:%._+~#=]{2,256}.[a-z]{2,4}\b([-a-zA-Z0-9()@:%_+.\`?$//=]*)" url > links



